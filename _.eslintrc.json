{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "valid-jsdoc":"warn",
        "require-jsdoc":"warn",
        "prefer-rest-params":"warn"
    }
}
